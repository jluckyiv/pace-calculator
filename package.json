{
  "name": "pace-calculator",
  "version": "1.0.0",
  "description": "A converter for running paces.",
  "bin": {
    "rpace": "bin.js"
  },
  "scripts": {
    "postinstall": "elm-tooling install",
    "build": "elm make --optimize src/Main.elm --output=src/main.js",
    "dev": "elm make src/Main.elm --output=src/main.js",
    "test": "uvu -r esm tests",
    "watch": "watchlist src tests -- pnpm test",
    "elm:test": "elm-test-rs",
    "elm:watch": "elm-test-rs --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jluckyiv/pace-calculator.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jluckyiv/pace-calculator/issues"
  },
  "homepage": "https://github.com/jluckyiv/pace-calculator#readme",
  "dependencies": {
    "elm-tooling": "^1.7.0",
    "sade": "^1.8.1"
  },
  "devDependencies": {
    "elm-test": "^0.19.1-revision7",
    "esm": "^3.2.25",
    "uvu": "^0.5.3",
    "watchlist": "^0.3.1"
  }
}
